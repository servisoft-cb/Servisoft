procedure TDmCons.qCReceberParcCalcFields(DataSet: TDataSet);
begin
  if (DmCons.qCReceberParcQUITADO.AsString = 'S') then
    DmCons.qCReceberParcclDiasAtraso.AsFloat := DmCons.qCReceberParcDIASATRASO.AsFloat
  else
  if (DmCons.qCReceberParcQUITADO.AsString = 'N') then
    begin
      if Date > DmCons.qCReceberParcDTVENCIMENTO.AsDateTime then
        DmCons.qCReceberParcclDiasAtraso.AsFloat := Date - DmCons.qCReceberParcDTVENCIMENTO.AsDateTime
      else
        DmCons.qCReceberParcclDiasAtraso.AsFloat := 0;
    end;
end;

procedure TDmCons.qCPagarParcCalcFields(DataSet: TDataSet);
